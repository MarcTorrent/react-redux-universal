{
	"verbose": true,
	"testPathDirs": [
		"<rootDir>/spec/"
	],
	"moduleDirectories": [
		"node_modules",
		"src/js"
	],
	"scriptPreprocessor": "<rootDir>/spec/support/jest-preprocessor",
	"setupFiles": [
		"<rootDir>/spec/support/tests-setup.js"
	],
	"coverageDirectory": "<rootDir>/reports/coverage",
	"coveragePathIgnorePatterns": [
		"/node_modules/",
		"<rootDir>/spec/support"
	]
}
