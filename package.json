{
  "name": "react-redux-universal",
  "version": "0.0.1",
  "description": "Boilerplate for ReactJS + Redux Universal Web Apps",
  "main": "index.js",
  "scripts": {
    "git-hooks": "cp ./tools/git-hooks/pre-commit ./.git/hooks/ && chmod 755 ./.git/hooks/pre-commit",
    "dev": "NODE_ENV=local nodemon ./src/server",
    "start": "npm run clean && npm run build && NODE_ENV=production node ./build/server",
    "start:client": "./node_modules/.bin/webpack-dev-server --config webpack.config.dev.js",
    "test": "jest --watch --onlyChanged",
    "test:watch": "jest --watch --onlyChanged",
    "validate": "eslint ./src/",
    "build": "webpack -p --config ./webpack.config.prod.js && webpack -p --config ./webpack.server.config.prod.js",
    "build:client": "webpack -p --config ./webpack.config.prod.js",
    "build:server": "webpack -p --config ./webpack.server.config.prod.js",
    "clean": "rm -rf build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MarcTorrent/react-redux-universal.git"
  },
  "keywords": [
    "react",
    "redux",
    "isomorphic",
    "universal",
    "redux-thunk",
    "redux-saga"
  ],
  "author": "Marc Torrent Vernetta",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MarcTorrent/react-redux-universal/issues"
  },
  "homepage": "https://github.com/MarcTorrent/react-redux-universal",
  "dependencies": {
    "babel-register": "^6.18.0",
    "body-parser": "^1.15.2",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.3",
    "express": "^4.14.0",
    "helmet": "^3.0.0",
    "hpp": "^0.2.1",
    "morgan": "^1.7.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-helmet": "^3.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0",
    "redial": "^0.5.0",
    "redux": "^3.6.0",
    "redux-saga": "^0.12.0",
    "whatwg-fetch": "^1.0.0"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.0",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.0",
    "babel-jest": "^16.0.0",
    "babel-loader": "^6.2.7",
    "babel-polyfill": "^6.16.0",
    "babel-preset-airbnb": "^2.1.1",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "chai": "^3.5.0",
    "cpy-cli": "^1.0.1",
    "css-loader": "^0.25.0",
    "enzyme": "^2.5.1",
    "es6-promise": "^4.0.5",
    "eslint": "^3.9.0",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.4.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "imports-loader": "^0.6.5",
    "jest": "^16.0.2",
    "jest-cli": "^16.0.2",
    "json-loader": "^0.5.4",
    "nock": "^9.0.2",
    "node-fetch": "^1.6.3",
    "node-sass": "^3.10.1",
    "react-addons-test-utils": "^15.3.2",
    "redux-logger": "^2.7.2",
    "redux-mock-store": "^1.2.1",
    "redux-thunk": "^2.1.0",
    "sass-loader": "^4.0.2",
    "sinon": "^1.17.6",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.3",
    "webpack-dashboard": "^0.2.0",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-dev-server": "^1.16.2",
    "webpack-hot-middleware": "^2.13.1",
    "webpack-manifest-plugin": "^1.1.0"
  },
  "jest": {
    "verbose": true,
    "testPathDirs": [
      "<rootDir>/spec/"
    ],
    "moduleDirectories": [
      "node_modules",
      "src/js"
    ],
    "scriptPreprocessor": "<rootDir>/jest-preprocessor",
    "setupFiles": [
      "<rootDir>/spec/support/tests-setup.js"
    ]
  }
}
